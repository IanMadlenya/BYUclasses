    RowBox[{
     RowBox[{"-", 
      FractionBox["\[Gamma]", 
       SuperscriptBox["R", "2"]]}], " ", "=", "  ", 
     RowBox[{
      RowBox[{"-", " ", "\[Gamma]"}], " ", 
      SuperscriptBox["u", "2"]}]}]}]}], TraditionalForm]], "Equation",
