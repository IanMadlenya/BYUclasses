     RowBox[{"(", 
      RowBox[{"1", " ", "-", " ", "cos\[Theta]"}], ")"}]}], " ", "+", " ", 
    RowBox[{"sin", " ", "\[Theta]"}]}], " ", "=", " ", 
   RowBox[{
    RowBox[{"0", " ", "\[Rule]", " ", 
     RowBox[{
      RowBox[{
       FractionBox["\[Gamma]", 
        RowBox[{"p", " ", 
         SuperscriptBox["V", "2"]}]], 
       RowBox[{"(", 
