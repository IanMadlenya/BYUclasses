Cell["\<\
As we can see, \[Theta] = 180\[Degree] is an equilibrium position. We will \
now check stability by looking at \
V\[CloseCurlyQuote]\[CloseCurlyQuote](\[Pi]) and see what the 2nd derivative \
is. We will also solve V\[CloseCurlyQuote]\[CloseCurlyQuote](\[Pi]) = 0 for \
\[Alpha].\
\>", "Text",
 CellChangeTimes->{{3.557150378465242*^9, 3.557150429843544*^9}, {
  3.557150571809649*^9, 3.557150586258766*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"(", 
   RowBox[{"Vprime2", "/.", " ", 
    RowBox[{"\[Theta]", " ", "\[Rule]", " ", "\[Pi]"}]}], ")"}], "//", 
  "FullSimplify"}], "\n", 
 RowBox[{"alpha", " ", "=", " ", 
  RowBox[{"\[Alpha]", "/.", " ", 
   RowBox[{
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"Vprime2", " ", "/.", " ", 
         RowBox[{"\[Theta]", " ", "\[Rule]", " ", "\[Pi]"}]}], ")"}], " ", 
       "\[Equal]", "0"}], " ", ",", " ", "\[Alpha]"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}]}]}], "Code",
 CellChangeTimes->{{3.5571504344138308`*^9, 3.557150470539915*^9}, {
  3.55715055753916*^9, 3.557150628043841*^9}, {3.557150701268745*^9, 
  3.557150707803087*^9}, {3.557151533441053*^9, 3.557151549138229*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"a", " ", "g", " ", "m"}], "-", 
   FractionBox[
    RowBox[{
     SuperscriptBox["a", "2"], " ", "\[Alpha]"}], "4"]}], 
  TraditionalForm]], "Output",
 CellChangeTimes->{
  3.557151362184976*^9, 3.557151436783925*^9, {3.557151500822075*^9, 
   3.557151542638833*^9}}],

