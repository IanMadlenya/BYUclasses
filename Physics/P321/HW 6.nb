Cell[CellGroupData[{

Cell["(ii) Springs in Series", "Subsection",
 CellChangeTimes->{{3.556549214129876*^9, 3.556549223661996*^9}}],

Cell[TextData[{
 "We will define S to be the total tension of the two springs when the mass \
is extended ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["x", "1"], "and", " ", 
    SubscriptBox["x", "2"]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ", by each spring idenpendently."
}], "Text",
 CellChangeTimes->{3.5565493032844677`*^9}],

Cell["\<\
With that in mind we can write the following equation: \
\>", "Text",
 CellChangeTimes->{{3.556549304269611*^9, 3.556549450358324*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["\[Alpha]", "series"], " ", "=", " ", 
   RowBox[{
    FractionBox["S", 
     RowBox[{
      SubscriptBox["x", "1"], "+", " ", 
      SubscriptBox["x", "2"]}]], " ", "=", " ", 
    RowBox[{
     FractionBox["S", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"S", "/", 
         SubscriptBox["\[Alpha]", "1"]}], ")"}], " ", "+", " ", 
       RowBox[{"(", 
        RowBox[{"S", "/", 
         SubscriptBox["\[Alpha]", "2"]}], ")"}]}]], " ", "=", " ", 
     RowBox[{
      FractionBox[
       RowBox[{
        SubscriptBox["\[Alpha]", "1"], 
        SubscriptBox["\[Alpha]", "2"], "S"}], 
       RowBox[{
        FractionBox[
         RowBox[{
          SubscriptBox["\[Alpha]", "2"], 
          SubscriptBox["\[Alpha]", "1"], "S"}], 
         SubscriptBox["\[Alpha]", "1"]], " ", "+", " ", 
        FractionBox[
         RowBox[{
          SubscriptBox["\[Alpha]", "2"], 
          SubscriptBox["\[Alpha]", "1"], "S"}], 
         SubscriptBox["\[Alpha]", "2"]]}]], " ", "=", " ", 
      RowBox[{
       FractionBox[
        RowBox[{
         SubscriptBox["\[Alpha]", "1"], 
         SubscriptBox["\[Alpha]", "2"], "S"}], 
        RowBox[{
         RowBox[{
          SubscriptBox["\[Alpha]", "1"], "S"}], " ", "+", " ", 
         RowBox[{
          SubscriptBox["\[Alpha]", "1"], "S"}]}]], " ", "=", " ", 
       RowBox[{
        FractionBox[
         RowBox[{
          SubscriptBox["\[Alpha]", "1"], 
          SubscriptBox["\[Alpha]", "2"]}], 
         RowBox[{
          SubscriptBox["\[Alpha]", "1"], " ", "+", " ", 
          SubscriptBox["\[Alpha]", "2"]}]], "=", "  ", 
        RowBox[{
         FractionBox[
          RowBox[{"m", " ", 
           SuperscriptBox[
            SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "m", " ", 
           SuperscriptBox[
            SubscriptBox["\[CapitalOmega]", "2"], "2"]}], 
          RowBox[{
           SuperscriptBox[
            SubscriptBox["m\[CapitalOmega]", "1"], "2"], " ", "+", " ", 
           SuperscriptBox[
            SubscriptBox["m\[CapitalOmega]", "2"], "2"]}]], " ", "=", " ", 
         FractionBox[
          RowBox[{"m", " ", 
           SuperscriptBox[
            SubscriptBox["\[CapitalOmega]", "1"], "2"], 
           SuperscriptBox[
            SubscriptBox["\[CapitalOmega]", "2"], "2"]}], 
          RowBox[{
           SuperscriptBox[
            SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "+", 
           SuperscriptBox[
            SubscriptBox["\[CapitalOmega]", "2"], "2"]}]]}]}]}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.5565494524986753`*^9, 3.556549532442896*^9}}],

Cell[TextData[{
 "We can now replace the left hand side with m ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox["\[CapitalOmega]", "series"], "2"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " and get the following:"
}], "Text",
 CellChangeTimes->{{3.556549602745739*^9, 3.556549638116015*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"m", 
    FormBox[
     SuperscriptBox[
      SubscriptBox["\[CapitalOmega]", "series"], "2"],
     TraditionalForm]}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     FractionBox[
      RowBox[{"m", " ", 
       SuperscriptBox[
        SubscriptBox["\[CapitalOmega]", "1"], "2"], 
       SuperscriptBox[
        SubscriptBox["\[CapitalOmega]", "2"], "2"]}], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "+", 
       SuperscriptBox[
        SubscriptBox["\[CapitalOmega]", "2"], "2"]}]], "\[Rule]", " ", 
     StyleBox[
      SubscriptBox["\[CapitalOmega]", "series"],
      FontSize->18,
      FontWeight->"Bold"]}], 
    StyleBox[" ",
     FontSize->18,
     FontWeight->"Bold"], 
    StyleBox["=",
     FontSize->18,
     FontWeight->"Bold"], 
    StyleBox[" ",
     FontSize->18,
     FontWeight->"Bold"], 
    StyleBox[
     RowBox[{
      StyleBox[
       FractionBox[
        RowBox[{
         SubscriptBox["\[CapitalOmega]", "1"], 
         SubscriptBox["\[CapitalOmega]", "2"]}], 
        SqrtBox[
         RowBox[{
          SuperscriptBox[
           SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "+", " ", 
          SuperscriptBox[
           SubscriptBox["\[CapitalOmega]", "2"], "2"]}]]],
       FontWeight->"Bold"], "\[Square]"}],
     FontSize->18]}]}], TraditionalForm]], "Equation",
 CellChangeTimes->{{3.556549647860881*^9, 3.5565496992430773`*^9}, {
  3.556550085179461*^9, 3.556550085868926*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell[TextData[{
 "Show that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[CapitalOmega]", "parallel"], "\[GreaterEqual]", 
    RowBox[{"2", " ", 
     SubscriptBox["\[CapitalOmega]", "series"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Subsection",
 CellChangeTimes->{{3.556549722827374*^9, 3.5565497540812683`*^9}}],

Cell[TextData[{
 "We can show this if the ratio ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     SubscriptBox["\[CapitalOmega]", "parallel"], 
     SubscriptBox["\[CapitalOmega]", "series"]], "\[GreaterEqual]", " ", 
    "2"}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 ". We will do that below (note that I replace parallel with p, and series \
with s)."
}], "Text",
 CellChangeTimes->{{3.5565497552728567`*^9, 3.556549804861952*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    SubscriptBox["\[CapitalOmega]", "p"], 
    SubscriptBox["\[CapitalOmega]", "s"]], " ", "=", " ", 
   RowBox[{
    FractionBox[
     SqrtBox[
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "+", " ", 
       SuperscriptBox[
        SubscriptBox["\[CapitalOmega]", "2"], "2"]}]], 
     RowBox[{
      SubscriptBox["\[CapitalOmega]", "1"], 
      RowBox[{
       SubscriptBox["\[CapitalOmega]", "2"], "/", 
       SqrtBox[
        RowBox[{
         SuperscriptBox[
          SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "+", " ", 
         SuperscriptBox[
          SubscriptBox["\[CapitalOmega]", "2"], "2"]}]]}]}]], " ", "=", " ", 
    FractionBox[
     RowBox[{
      SuperscriptBox[
       SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "+", " ", 
      SuperscriptBox[
       SubscriptBox["\[CapitalOmega]", "2"], "2"]}], 
     RowBox[{
      SubscriptBox["\[CapitalOmega]", "1"], 
      SubscriptBox["\[CapitalOmega]", "2"]}]]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{{3.5565497805118027`*^9, 3.5565498747325687`*^9}}],

Cell["\<\
We now complete the square on the numerator to get:\
\>", "Text",
 CellChangeTimes->{{3.5565499039572973`*^9, 3.556549912663455*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   FractionBox[
    RowBox[{
     SuperscriptBox[
      SubscriptBox["\[CapitalOmega]", "1"], "2"], " ", "+", " ", 
     SuperscriptBox[
      SubscriptBox["\[CapitalOmega]", "2"], "2"], "-", 
     RowBox[{"2", " ", 
      SubscriptBox["\[CapitalOmega]", "2"], " ", 
      SubscriptBox["\[CapitalOmega]", "1"]}], " ", "+", 
     RowBox[{"2", " ", 
      SubscriptBox["\[CapitalOmega]", "2"], " ", 
      SubscriptBox["\[CapitalOmega]", "1"]}]}], 
    RowBox[{
     SubscriptBox["\[CapitalOmega]", "1"], 
