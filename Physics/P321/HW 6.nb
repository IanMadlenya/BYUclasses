Cell[CellGroupData[{

Cell["P5.5", "Section",
 CellChangeTimes->{{3.556548569126404*^9, 3.55654857150979*^9}}],

Cell[TextData[{
 "The differential equation governing the motion of the system is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      OverscriptBox["x", ".."], " ", "+", " ", 
      RowBox[{"2", " ", "K", " ", 
       OverscriptBox["x", "."]}], " ", "+", " ", 
      RowBox[{
       SuperscriptBox["\[CapitalOmega]", "2"], "x"}]}], " ", "=", " ", "0"}], 
    ",", " ", 
    RowBox[{
     RowBox[{"v", "(", "0", ")"}], " ", "=", " ", "0"}], ",", " ", 
    RowBox[{
     RowBox[{"x", "(", "0", ")"}], "=", " ", "a"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " with K < \[CapitalOmega]."
}], "Text",
 CellChangeTimes->{{3.556550371565175*^9, 3.5565503755070066`*^9}}],

Cell["\<\
That means out system is under-damped.\
\>", "Text",
 CellChangeTimes->{{3.556550375829171*^9, 3.5565503843106127`*^9}}],

Cell["\<\
Following the example on page 110 of the text, equation (5.10) gives us the \
real general solution to the equation is:\
\>", "Text",
 CellChangeTimes->{{3.556550384597247*^9, 3.556550423561954*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"x", " ", "=", " ", 
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"-", "K"}], " ", "t"}]], "(", 
     RowBox[{
      RowBox[{"A", " ", "cos", " ", 
       SubscriptBox["\[CapitalOmega]", "D"], "t"}], " ", "+", " ", 
      RowBox[{"B", " ", "sin", " ", 
       SubscriptBox["\[CapitalOmega]", "D"], "t"}]}], ")"}]}], ",", " ", 
   RowBox[{
    RowBox[{"where", " ", 
     SubscriptBox["\[CapitalOmega]", "D"]}], " ", "=", " ", 
    SqrtBox[
     RowBox[{
      SuperscriptBox["\[CapitalOmega]", "2"], " ", "-", " ", 
      SuperscriptBox["K", "2"]}]]}]}], TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.5565504258047733`*^9, 3.556550443723403*^9}, {
  3.556551106784999*^9, 3.556551132325321*^9}}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   OverscriptBox["x", "."], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " can be given to us by ",
 StyleBox["Mathematica ",
  FontSlant->"Italic"],
 "(note that we use a lower case \[OpenCurlyQuote]k\[CloseCurlyQuote] to \
represent K, and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[CapitalOmega]", "D"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " is simply \[CapitalOmega]):"
}], "Text",
 CellChangeTimes->{{3.556550465777503*^9, 3.55655050406074*^9}, {
  3.556551050106141*^9, 3.556551069990397*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"x", "[", "t_", "]"}], " ", ":=", " ", 
   RowBox[{
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", " ", "k"}], " ", "t"}], "]"}], " ", "*", 
    RowBox[{"(", " ", 
     RowBox[{
      RowBox[{"A", " ", 
       RowBox[{"Cos", "[", 
        RowBox[{"\[CapitalOmega]", " ", "t"}], "]"}]}], " ", "+", " ", 
      RowBox[{"B", " ", 
       RowBox[{"Sin", "[", 
        RowBox[{"\[CapitalOmega]", " ", "t"}], "]"}]}]}], ")"}]}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"xDot", "[", "t_", "]"}], " ", ":=", " ", 
   RowBox[{"Evaluate", "[", 
    RowBox[{"D", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", " ", "k"}], " ", "t"}], "]"}], " ", "*", 
       RowBox[{"(", " ", 
        RowBox[{
         RowBox[{"A", " ", 
          RowBox[{"Cos", "[", 
           RowBox[{"\[CapitalOmega]", " ", "t"}], "]"}]}], " ", "+", " ", 
         RowBox[{"B", " ", 
          RowBox[{"Sin", "[", 
           RowBox[{"\[CapitalOmega]", " ", "t"}], "]"}]}]}], ")"}]}], ",", 
      " ", "t"}], "]"}], "]"}]}], ";", " ", 
  RowBox[{
   RowBox[{"xDot", "[", "t", "]"}], "//", "FullSimplify"}]}]}], "Code",
 CellChangeTimes->{{3.556550505799303*^9, 3.556550591543476*^9}, {
  3.5565506328114023`*^9, 3.556550664276923*^9}, {3.556550709630571*^9, 
  3.556550715412621*^9}, {3.556551355360133*^9, 3.5565513574203367`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{
     RowBox[{"-", "k"}], " ", "t"}]], " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{
      RowBox[{"cos", "(", 
       RowBox[{"t", " ", "\[CapitalOmega]"}], ")"}], " ", 
