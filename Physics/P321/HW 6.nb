   3.556550715884766*^9, 3.556550797551238*^9, 3.556551358420175*^9, 
   3.556553682599638*^9}]
}, Open  ]],

Cell["\<\
We can use these two equations as well as the two first order conditions to \
solve for A and B:\
\>", "Text",
 CellChangeTimes->{{3.556550595704894*^9, 3.5565506233026533`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"sol", " ", "=", " ", 
   RowBox[{"Solve", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"x", "[", "0", "]"}], " ", "\[Equal]", " ", "a"}], ",", " ", 
       RowBox[{
        RowBox[{"xDot", "[", "0", "]"}], " ", "\[Equal]", " ", "0"}]}], "}"}],
      ",", " ", 
     RowBox[{"{", 
      RowBox[{"A", ",", " ", "B"}], "}"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"c1", " ", "=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"A", " ", "/.", " ", "sol"}], ")"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"c2", " ", "=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"B", " ", "/.", " ", "sol"}], ")"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{"Print", "[", 
  RowBox[{
  "\"\<A = \>\"", ",", " ", "c1", ",", " ", "\"\<    and    B = \>\"", ",", 
   " ", "c2"}], "]"}]}], "Code",
 CellChangeTimes->{{3.556550623985684*^9, 3.5565506268056192`*^9}, {
  3.556550668882565*^9, 3.556550690331438*^9}, {3.556550748607112*^9, 
  3.556550848438279*^9}}],

Cell[BoxData[
 FormBox[
  InterpretationBox[
   RowBox[{"\<\"A = \"\>", "\[InvisibleSpace]", "a", 
    "\[InvisibleSpace]", "\<\"    and    B = \"\>", "\[InvisibleSpace]", 
    FractionBox[
     RowBox[{"a", " ", "k"}], "\[CapitalOmega]"]}],
   SequenceForm[
