     FontSize->16,
     FontWeight->"Bold"], 
    RowBox[{
     StyleBox[
      RowBox[{
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[CapitalOmega]", "1"], " ", "-", " ", 
           SubscriptBox["\[CapitalOmega]", "2"]}], ")"}], "2"], 
        RowBox[{
         SubscriptBox["\[CapitalOmega]", "2"], " ", 
         SubscriptBox["\[CapitalOmega]", "1"]}]], " ", "+", " ", "2"}],
      FontSize->16,
      FontWeight->"Bold"], 
     StyleBox[" ",
