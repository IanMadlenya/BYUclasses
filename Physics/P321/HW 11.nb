Cell[CellGroupData[{

Cell["P7.4", "Section",
 CellChangeTimes->{{3.557329179237965*^9, 3.557329180636994*^9}}],

Cell["\<\
We know that in a conservative field F = - \[Del]V. In this case, that makes:\
\
\>", "Text",
 CellChangeTimes->{{3.5574854654135323`*^9, 3.557485563218073*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{"V", "=", 
   RowBox[{
    RowBox[{"-", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"F", "(", "r", ")"}], 
       RowBox[{"\[DifferentialD]", "r"}]}]}]}], " ", "=", 
    RowBox[{
     RowBox[{
      RowBox[{"-", 
       RowBox[{"(", "-", ")"}]}], "m", " ", 
      SuperscriptBox["\[CapitalOmega]", "2"], 
      RowBox[{"\[Integral]", 
       RowBox[{"r", " ", 
        RowBox[{"\[DifferentialD]", "r"}]}]}]}], " ", "=", 
     RowBox[{
      FormBox[
       RowBox[{"m", " ", 
        SuperscriptBox["\[CapitalOmega]", "2"], " ", 
        FractionBox[
         SuperscriptBox["r", "2"], "2"]}],
       TraditionalForm], "\[Rule]", " ", 
      RowBox[{
       SuperscriptBox["\[CapitalOmega]", "2"], 
       FractionBox[
        SuperscriptBox["r", "2"], "2"]}]}]}]}]}], 
  TraditionalForm]], "Equation",
 CellChangeTimes->{
  3.557485565870728*^9, {3.55748612785115*^9, 3.5574861395468063`*^9}}],

Cell["\<\
The last part of the previous line includes Gregory\[CloseCurlyQuote]s \
\[OpenCurlyQuote]massless\[CloseCurlyQuote] equation for V and L.\
\>", "Text",
 CellChangeTimes->{{3.557486143118541*^9, 3.557486168047345*^9}}],

Cell["\<\
Using equations (7.5) and (7.7) we can write the angular momentum equation \
and Energy equation\
\>", "Text",
 CellChangeTimes->{{3.557485651918488*^9, 3.5574856769919043`*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SuperscriptBox["r", "2"], " ", 
    OverscriptBox["\[Theta]", "."]}], " ", "=", " ", "L"}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.557485683298911*^9, 3.557485692088922*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        OverscriptBox["r", "."], "2"], " ", "+", " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"r", " ", 
          OverscriptBox["\[Theta]", "."]}], ")"}], "2"]}], ")"}]}], " ", "+", 
    "  ", 
    RowBox[{
     SuperscriptBox["\[CapitalOmega]", "2"], " ", 
     FractionBox[
      SuperscriptBox["r", "2"], "2"]}]}], " ", "=", " ", "E"}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.5574856987770557`*^9, 3.5574857337854137`*^9}, 
   3.557486126794722*^9}],

Cell[TextData[{
 "We solve for ",
 Cell[BoxData[
  FormBox[
   OverscriptBox["\[Theta]", "."], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " in equation (0.1) and plug it in to (0.2) to get the following"
}], "Text",
 CellChangeTimes->{{3.557485779304016*^9, 3.557485803141177*^9}}],

Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{
     FractionBox["1", "2"], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        OverscriptBox["r", "."], "2"], " ", "+", " ", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"r", " ", 
          FractionBox["L", 
           SuperscriptBox["r", "2"]]}], ")"}], "2"]}], ")"}]}], " ", "+", " ", 
    RowBox[{"m", " ", 
     SuperscriptBox["\[CapitalOmega]", "2"], " ", 
     FractionBox[
      SuperscriptBox["r", "2"], "2"]}]}], " ", "=", " ", 
   RowBox[{"E", " ", "\[Rule]", " ", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      SuperscriptBox[
       OverscriptBox["r", "."], "2"]}], " ", "+", " ", 
     FractionBox[
      SuperscriptBox["L", "2"], 
      RowBox[{"2", " ", 
       SuperscriptBox["r", "2"]}]], " ", "+", " "}]}]}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.557485806936392*^9, 3.557485876504366*^9}, {
