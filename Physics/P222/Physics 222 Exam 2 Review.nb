 CellChangeTimes->{{3.532269159633962*^9, 3.532269182080962*^9}, {
   3.532269295530962*^9, 3.532269398626962*^9}, {3.532269627285962*^9, 
   3.532269640584962*^9}, 3.532269676275962*^9}],

Cell["\<\
Are the most easily stopped as they pass through matter (can be stopped by a \
single sheet of paper). \
\>", "Item2",
 CellChangeTimes->{{3.532269159633962*^9, 3.532269182080962*^9}, {
  3.532269295530962*^9, 3.532269398626962*^9}, {3.532269627285962*^9, 
  3.532269643832962*^9}, {3.532269677554962*^9, 3.5322696781949625`*^9}}],

Cell["They carry a positive charge. ", "Item2",
 CellChangeTimes->{{3.532269159633962*^9, 3.532269182080962*^9}, {
  3.532269295530962*^9, 3.532269398626962*^9}, {3.532269627285962*^9, 
  3.532269645928962*^9}}],

Cell[TextData[{
 "They are, pretty much, just a ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["He", "++"], TraditionalForm]],
  FormatType->"TraditionalForm"],
 " nucleus. "
}], "Item2",
 CellChangeTimes->{{3.532269159633962*^9, 3.532269182080962*^9}, {
  3.532269295530962*^9, 3.532269398626962*^9}, {3.532269627285962*^9, 
  3.532269645924962*^9}}]
