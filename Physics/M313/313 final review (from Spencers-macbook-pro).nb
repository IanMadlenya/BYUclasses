Cell[TextData[{
 "k",
 StyleBox["(u \[Cross] v)=(",
  FontWeight->"Bold"],
 "k",
 StyleBox["u) \[Cross] v=u \[Cross] (",
  FontWeight->"Bold"],
 "k",
