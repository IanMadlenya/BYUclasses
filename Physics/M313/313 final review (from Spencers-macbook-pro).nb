 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["A", "T"], "A", " ", "have", " ", "the", " ", "same", " ",
      "null", " ", "space"}], ",", " ", 
    RowBox[{"row", " ", "space"}], ",", " ", 
    RowBox[{"and", " ", 
     RowBox[{"rank", ".", "\[IndentingNewLine]", 
      SuperscriptBox["A", "T"]}], "A", " ", "and", " ", 
     SuperscriptBox["A", "T"], " ", "have", " ", "the", " ", "same", " ", 
     "column", " ", 
