  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["A", "T"], "A", " ", "is", " ", "orthogonally", " ", 
     "diagonalizeable", " ", "and", " ", "the", " ", "eigenvalues", " ", "of",
      " ", 
     SuperscriptBox["A", "T"], "A", " ", "are", " ", "non"}], "-", 
