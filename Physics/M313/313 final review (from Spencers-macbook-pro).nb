}], "Subitem",
 CellChangeTimes->{{3.511822577510359*^9, 3.511822614283588*^9}, {
  3.511959844672686*^9, 3.511959885006422*^9}}],

Cell["Theorem 1.4.4: if B and C are both inverses of A then B=C", "Subitem",
 CellChangeTimes->{{3.5118226145735903`*^9, 3.511822629397299*^9}, {
   3.511822680332149*^9, 3.5118226803365707`*^9}, 3.51195988967848*^9}],

Cell[TextData[{
 "Theorem 1.4.6: If A and B are invertible matrices of the same size then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", "AB", ")"}], 
     RowBox[{"-", "1"}]], "=", 
    RowBox[{
     SuperscriptBox["B", 
      RowBox[{"-", "1"}]], 
     SuperscriptBox["A", 
      RowBox[{"-", "1"}]]}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511822723539826*^9, 3.511822726944395*^9}, {
  3.511959891062606*^9, 3.511959892110784*^9}}],

Cell[TextData[{
 "Theorem 1.4.7: ",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"if", " ", "A", " ", "is", " ", "invertible", " ", 
      RowBox[{"(", 
       RowBox[{"non", "-", "singular"}], ")"}], " ", "and", " ", "n", " ", 
      "is", " ", "a", " ", "non"}], "-", 
     RowBox[{"negative", " ", "interger"}]}], ",", 
    RowBox[{"then", ":", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"a", ".", " ", 
       SuperscriptBox["A", 
        RowBox[{"-", "1", " "}]]}], "is", " ", "invertibe", " ", "and", " ", 
      SuperscriptBox[
       RowBox[{"(", 
        SuperscriptBox["A", 
         RowBox[{"-", "1"}]], ")"}], 
       RowBox[{"-", "1"}]], "isA"}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"b", ".", " ", 
      SuperscriptBox["A", "n"]}], " ", "is", " ", "invertible", " ", "and", 
     " ", 
     SuperscriptBox[
      RowBox[{"(", 
       SuperscriptBox["A", "n"], ")"}], 
      RowBox[{"-", "1"}]]}], "=", 
    RowBox[{
     SuperscriptBox["A", 
      RowBox[{"-", "n"}]], "=", 
     SuperscriptBox[
      RowBox[{"(", 
       SuperscriptBox["A", 
        RowBox[{"-", "1"}]], ")"}], "n"]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"c", ".", " ", "kA"}], " ", "is", " ", "invertible", " ", "for", 
     " ", "any", " ", "nonzero", " ", "scalar", " ", "k"}], ",", " ", 
    RowBox[{
     RowBox[{"and", " ", 
      SuperscriptBox[
       RowBox[{"(", "kA", ")"}], 
       RowBox[{"-", "1"}]]}], "=", 
     RowBox[{
      SuperscriptBox["k", 
       RowBox[{"-", "1"}]], 
      SuperscriptBox["A", 
       RowBox[{"-", "1"}]]}]}]}], TraditionalForm]}]]
}], "Subitem",
 CellChangeTimes->{{3.511822727243251*^9, 3.51182290960918*^9}}],

Cell[TextData[{
 "Theorem 1.4.8: ",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
    "If", " ", "the", " ", "sizes", " ", "of", " ", "teh", " ", "matrices", 
     " ", "are", " ", "such", " ", "that", " ", "the", " ", "stated", " ", 
     "operations", " ", "can", " ", "be", " ", "performed"}], ",", 
    RowBox[{
     RowBox[{"then", ":", "\[IndentingNewLine]", 
      RowBox[{"a", ".", " ", 
       SuperscriptBox[
        RowBox[{"(", 
         SuperscriptBox["A", "T"], ")"}], "T"]}]}], "=", "A"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"b", ".", " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"A", "+", "B"}], ")"}], "T"]}], "=", 
    RowBox[{
     SuperscriptBox["A", "T"], "+", 
     SuperscriptBox["B", "T"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"c", ".", " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"A", "-", "B"}], ")"}], "T"]}], "=", 
    RowBox[{
     SuperscriptBox["A", "T"], "-", 
     SuperscriptBox["B", "T"]}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"d", ".", " ", 
     SuperscriptBox[
      RowBox[{"(", "kA", ")"}], "T"]}], "=", 
    SuperscriptBox["kA", "T"]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"e", ".", " ", 
     SuperscriptBox[
      RowBox[{"(", "AB", ")"}], "T"]}], "=", 
    RowBox[{
     SuperscriptBox["B", "T"], 
     SuperscriptBox["A", "T"]}]}], TraditionalForm]}]]
}], "Subitem",
 CellChangeTimes->{{3.51182289578335*^9, 3.511823067609042*^9}}],

Cell[TextData[{
 "Theorem 1.4.9: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
    "If", " ", "A", " ", "is", " ", "an", " ", "invertible", " ", "matrix"}], 
    ",", " ", 
    RowBox[{
     RowBox[{"then", " ", 
      SuperscriptBox["A", "T"], "is", " ", "also", " ", "invertible", " ", 
      RowBox[{"and", ":", "\[IndentingNewLine]", 
       SuperscriptBox[
        RowBox[{"(", 
         SuperscriptBox["A", "T"], ")"}], 
        RowBox[{"-", "1"}]]}]}], "=", 
     SuperscriptBox[
      RowBox[{"(", 
       SuperscriptBox["A", 
        RowBox[{"-", "1"}]], ")"}], "T"]}]}], TraditionalForm]]],
 " "
}], "Subitem",
 CellChangeTimes->{{3.51182289578335*^9, 3.511823127511232*^9}}],

Cell[TextData[{
 "Theorem 1.5.1: If the elementary matrix E is obtained by performing a \
certain elementary row operation on ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["I", "m"], " ", "and", " ", "if", " ", "A", " ", "is", " ", 
    RowBox[{"m", " ", "\[Cross]", " ", "n", " "}]}], TraditionalForm]]],
 ",then the product EA is the matrix that results when the same row operation \
is performed on A. "
}], "Subitem",
 CellChangeTimes->{{3.511823233798966*^9, 3.511823311455044*^9}}],

Cell["\<\
Theorem 1.5.2: Every elementary matrix is invertible and its inverse is also \
an elementary matrix. \
\>", "Subitem",
 CellChangeTimes->{{3.511823332078637*^9, 3.5118233620133047`*^9}, {
  3.511823501106855*^9, 3.511823501111722*^9}}],

Cell[TextData[{
 "Theorem 1.6.2: If A is an invertible matrix then the system Ax=b has \
exactly one solution, namely ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "=", 
    RowBox[{
     SuperscriptBox["A", 
      RowBox[{"-", "1"}]], "b"}]}], TraditionalForm]]],
 " "
}], "Subitem",
 CellChangeTimes->{{3.5118235013547783`*^9, 3.5118235360046*^9}, {
   3.511823857822205*^9, 3.511823858145525*^9}, 3.511959892934778*^9}],

Cell["\<\
Theorem 1.6.5: Let A and B by square matrices of the same size. If AB is \
invertible, then A and B must also be invertible. \
\>", "Subitem",
 CellChangeTimes->{{3.51182385851544*^9, 3.511823893690329*^9}, {
   3.511824029287745*^9, 3.5118240292924232`*^9}, 3.5119598937984667`*^9}],

Cell[TextData[{
 "Theorem 1.7.1: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"a", ".", " ", "The"}], " ", "transpose", " ", "of", " ", "an",
        " ", "upper", " ", 
       RowBox[{"(", "lower", ")"}], " ", "triangular", " ", "matrix", " ", 
       "is", " ", "lower", " ", 
       RowBox[{"(", "upper", ")"}], " ", "triangular"}], " ", "-", " ", 
      RowBox[{"it", " ", "switches"}]}], " ", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"b", ".", " ", "The"}], " ", "product", " ", "of", " ", "lower",
       " ", 
      RowBox[{"(", "upper", ")"}], " ", "triangular", " ", "matricies", " ", 
      "is", " ", "lower", " ", 
      RowBox[{"(", "upper", ")"}], " ", "triangular"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"c", ".", " ", "A"}], " ", "trianglular", " ", "matrix", " ", 
       "is", " ", "invertible", " ", "iff", " ", "its", " ", "diagonal", " ", 
       "entries", " ", "are", " ", "all", " ", "non"}], "-", "zero"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"d", ".", " ", "The"}], " ", "inverse", " ", "of", " ", "an", 
      " ", "invertible", " ", "lower", " ", 
      RowBox[{"(", "upper", ")"}], " ", "triangular", " ", "matrix", " ", 
      "is", " ", "lower", " ", 
      RowBox[{"(", "upper", ")"}], " ", 
      RowBox[{"triangular", ".", " "}]}]}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511824029487657*^9, 3.5118241359064693`*^9}, {
  3.511824174364622*^9, 3.5118241743701277`*^9}}],

Cell[TextData[{
 "Theorem 1.7.2 ",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
    "If", " ", "A", " ", "and", " ", "B", " ", "are", " ", "symmetric", " ", 
     "matricies", " ", "of", " ", "the", " ", "same", " ", "size"}], ",", " ", 
    RowBox[{
     RowBox[{
     "and", " ", "if", " ", "k", " ", "is", " ", "any", " ", "scalar", " ", 
      "then"}], ";"}]}], TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"a", ".", " ", 
     SuperscriptBox["A", "T"]}], " ", "is", " ", "symmetric"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"b", ".", " ", "A"}], "+", 
    RowBox[{"B", "  ", "and", " ", "A"}], "-", 
    RowBox[{"B", " ", "are", " ", "symmetric"}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{"c", ".", " ", "kA"}], " ", "is", " ", 
    RowBox[{"symmetric", ".", " "}]}], TraditionalForm]}]]
}], "Subitem",
 CellChangeTimes->{{3.511824174652582*^9, 3.511824257489667*^9}}],

Cell[TextData[{
 "Theorem 1.7.4: If A is an invertible symmetric matrix, then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["A", 
     RowBox[{"-", "1", " "}]], "is", " ", 
    RowBox[{"symmetric", "."}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511824265065477*^9, 3.51182429084811*^9}, 
   3.51195989603023*^9}],

Cell[TextData[{
 "Theorem 1.7.5: If A is an invertible matrix, then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["AA", "T"], " ", "and", " ", 
    SuperscriptBox["A", "T"], "A", " ", "are", " ", "also", " ", 
    RowBox[{"invertible", ".", " "}]}], TraditionalForm]]]
}], "Subitem",
