 CellChangeTimes->{{3.5118206350944757`*^9, 3.511820641182288*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "Theorem 6.2.1 Cauchy Schwartz Inequality: If u and v are vectors in a real \
inner product space V, then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"|", 
     RowBox[{"<", "u"}]}], ",", 
    RowBox[{
     RowBox[{"v", ">"}], "|", 
     RowBox[{"\[LessEqual]", 
      RowBox[{"||", "u", "||", " ", "||", "v", "||"}]}]}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.5118882245911617`*^9, 3.5118882994221373`*^9}, 
   3.511960103362503*^9, {3.511970843885796*^9, 3.511970846890881*^9}}],

Cell[TextData[{
 "Theorem 6.2.3: If u and v are vectors in an inner product space and are \
orthogonal then ",
 Cell[BoxData[
  FormBox[
   RowBox[{"||", 
    RowBox[{"u", "+", "v"}], 
    SuperscriptBox["||", "2"], 
    RowBox[{"+", 
     RowBox[{"||", "u", 
      SuperscriptBox["||", "2"], 
      RowBox[{"+", 
       RowBox[{"||", "v", 
        SuperscriptBox["||", "2"]}]}]}]}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511888299714946*^9, 3.511888300394631*^9}, {
  3.5118884082794323`*^9, 3.5118884447947083`*^9}, {3.511888549420527*^9, 
  3.511888549426374*^9}}],

Cell[TextData[{
 "Theorem 6.3.1: If ",
 Cell[BoxData[
  FormBox[
   RowBox[{"S", "=", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       SubscriptBox["v", "1"], ",", "...", " ", ",", " ", 
       SubscriptBox["v", "n"]}], "}"}], " ", "is", " ", "an", " "}]}], 
   TraditionalForm]]],
 "orthogonal set of non zero vectors in an inner product space, then S is \
linearly independent. "
}], "Subitem",
 CellChangeTimes->{{3.511888550042386*^9, 3.51188858980733*^9}, 
   3.511960108644758*^9}],

Cell[TextData[{
 "Theorem 6.3.2 If ",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{"S", "=", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        SubscriptBox["v", "1"], ",", "...", " ", ",", " ", 
        SubscriptBox["v", "n"]}], "}"}], " ", "is", " ", "an", " ", 
      "orthogoanl", " ", "basis", " ", "for", " ", "an", " ", "iner", " ", 
