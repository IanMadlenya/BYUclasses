Cell[CellGroupData[{

Cell[TextData[{
 "if ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"v", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["v", "1"], ",", 
        SubscriptBox["v", "2"], ",", "...", " ", ",", 
        SubscriptBox["v", "n"]}], ")"}], " ", "is", " ", "a", " ", "vector", 
      " ", "in", " ", 
      SuperscriptBox["R", "n"]}]}], ",", " ", 
    RowBox[{"then", " ", "the", " ", "norm", " ", "of", " ", "v", " ", 
     RowBox[{"(", 
      RowBox[{"also", " ", "called", " ", "length"}]}]}]}], TraditionalForm]]],
 " or magnitude of v) is denoted by ||",
 StyleBox["v",
  FontWeight->"Bold"],
 "||, and is defined by the formula ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"||", 
     StyleBox["v",
      FontWeight->"Bold"], "||"}], "=", 
    SqrtBox[
     RowBox[{
      RowBox[{
       SuperscriptBox[
        SubscriptBox["v", "1"], "2"], "+", 
       SuperscriptBox[
        SubscriptBox["v", "2"], "2"], "+"}], ",", ",", ",", 
      RowBox[{"+", 
       SuperscriptBox[
        SubscriptBox["v", "n"], "2"]}]}]]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511869643670841*^9, 3.511869747737554*^9}, {
  3.511869853592237*^9, 3.511869853599242*^9}}],

Cell[TextData[{
 "A unit vector is a vector with norm of one. They are computed by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["u",
     FontWeight->"Bold"], "=", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"||", 
       StyleBox["v",
        FontWeight->"Bold"], "||"}]], 
     StyleBox["v",
      FontWeight->"Bold"]}]}], TraditionalForm]]],
 ". "
}], "Subitem",
 CellChangeTimes->{{3.511869854016897*^9, 3.511869906246368*^9}}],

Cell[TextData[{
 "The distance formula in ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["R", "n"], " ", "is", " ", "the", " ", "following", " ", 
     RowBox[{"equation", ":", " ", 
      RowBox[{"d", "(", 
       StyleBox[
        RowBox[{"u", ",", "v"}],
        FontWeight->"Bold"], 
       StyleBox[")",
        FontWeight->"Bold"]}]}]}], 
    StyleBox["=",
     FontWeight->"Bold"], 
    RowBox[{
     StyleBox[
      RowBox[{"||", 
       RowBox[{"u", "-", "v"}], "||"}],
      FontWeight->"Bold"], 
     StyleBox["=",
      FontWeight->"Bold"], 
     SqrtBox[
      RowBox[{
       RowBox[{
        RowBox[{
         SuperscriptBox[
          RowBox[{
           StyleBox["(",
            FontWeight->"Bold"], 
           RowBox[{
            SubscriptBox["u", "1"], "-", 
            SubscriptBox["v", "1"]}], ")"}], "2"], "+", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SubscriptBox["u", "2"], "-", 
            SubscriptBox["v", "2"]}], ")"}], "2"], "+"}], "..."}], "+", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          SubscriptBox["u", "n"], "-", 
          SubscriptBox["v", "n"]}], ")"}], "2"]}]]}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511869906855589*^9, 3.511870000900394*^9}}],

Cell[TextData[{
 "The Euclidean inner product is the dot product and is ",
 StyleBox["u\[CenterDot]v=||u||*||v|| ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"cos", "[", "\[Theta]", "]"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        SubscriptBox["u", "1"], 
        SubscriptBox["v", "1"]}], "+"}], "..."}], "+", 
     RowBox[{
      SubscriptBox["u", "n"], 
      SubscriptBox["v", "n"]}]}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511870001749237*^9, 3.511870074147167*^9}}],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"||", "v", "||"}], "=", 
   SqrtBox[
    StyleBox[
     RowBox[{"v", "\[CenterDot]", "v"}],
     FontWeight->"Bold"]]}], TraditionalForm]]]], "Subitem",
 CellChangeTimes->{{3.5118700782279367`*^9, 3.511870091652873*^9}}]
}, Open  ]],

