Cell[TextData[{
 "Theorem 6.3.3 Projection Theorem: If W is a finite-dimensional subspace of \
an inner product space V, then every vector u in V can be expressed in \
exactly on e way as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "=", 
    RowBox[{
     SubscriptBox["w", "1"], "+", 
     SubscriptBox["w", "2"]}]}], TraditionalForm]]],
 " where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["w", "1"], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["proj", "W"], "u", " ", "and", " ", 
      SubscriptBox["w", "2"]}], "=", 
     RowBox[{
      SubscriptBox["proj", 
       SuperscriptBox["W", "\[Perpendicular]"]], "u"}]}]}], TraditionalForm]]],
 " = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"u", "-", 
    RowBox[{
     SubscriptBox["proj", "x"], "u"}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511888723924758*^9, 3.511888829495981*^9}, {
  3.511960111330823*^9, 3.51196011360287*^9}}],

