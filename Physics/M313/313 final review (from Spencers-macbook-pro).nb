Cell[TextData[{
 "There is an object ",
 StyleBox["0",
  FontWeight->"Bold"],
 " in V, called the zero vector for V, such that ",
 StyleBox["0 +u =u +0 =u", "Subitem",
  FontWeight->"Bold"],
 StyleBox[" for all u\[CloseCurlyQuote]s in V.", "Subitem"]
}], "ItemNumbered",
 CellChangeTimes->{{3.511874471229431*^9, 3.511874501666686*^9}, 
   3.511969089632863*^9, 3.511969687492651*^9}],

Cell[TextData[StyleBox["For each u in V, there is an object -u in V called a \
negative of v such that u=(-u)=0", "Subitem"]], "ItemNumbered",
 CellChangeTimes->{{3.5118745033884497`*^9, 3.5118745469534597`*^9}, {
   3.511969081210712*^9, 3.511969083848707*^9}, 3.511969688484283*^9}],

Cell[TextData[StyleBox["If k is any scalar and u is any object in V, then ku \
is in V", "Subitem"]], "ItemNumbered",
