Cell[CellGroupData[{

Cell["3.3 Orthogonality", "Subsection",
 CellChangeTimes->{{3.511820393883247*^9, 3.511820402667145*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "Two vectors are said to be orthogonal if ",
 StyleBox["u\[CenterDot]v",
  FontWeight->"Bold"],
 "=0. An orthogonal set is one i which each vector in the set is orthogonal \
to every other vector in the set. "
}], "Subitem",
 CellChangeTimes->{{3.511870448060698*^9, 3.5118704994332657`*^9}, 
   3.5119696635017433`*^9}],

Cell[TextData[{
 "Vector equation for a line: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"a", "(", 
      RowBox[{"x", "-", 
       SubscriptBox["x", "0"]}], ")"}], "+", 
     RowBox[{"b", "(", 
      RowBox[{"y", "-", 
       SubscriptBox["y", "0"]}], ")"}]}], "=", "0"}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.5118705003071127`*^9, 3.511870538834977*^9}}],

Cell[TextData[{
 "Vector equation for a plane: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"a", "(", 
      RowBox[{"x", "-", 
       SubscriptBox["x", "0"]}], ")"}], "+", 
     RowBox[{"b", "(", 
      RowBox[{"y", "-", 
       SubscriptBox["y", "0"]}], ")"}], "+", 
     RowBox[{"c", "(", 
      RowBox[{"z", "-", 
       SubscriptBox["z", "0"]}], ")"}]}], "=", "0"}], TraditionalForm]]],
 " (These two equations are also called the point-normal equations because \
they are made from a point and from the normal vector.)"
}], "Subitem",
 CellChangeTimes->{{3.5118705264023542`*^9, 3.511870595665066*^9}, 
   3.511969671301548*^9}],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["proj", "a"], 
     StyleBox["u",
      FontWeight->"Bold"]}], 
    StyleBox["=",
     FontWeight->"Bold"], 
    RowBox[{
     FractionBox[
      StyleBox[
       RowBox[{"u", "\[CenterDot]", "a"}],
       FontWeight->"Bold"], 
      RowBox[{"||", 
       StyleBox["a",
        FontWeight->"Bold"], 
       SuperscriptBox[
        StyleBox["||",
         FontWeight->"Bold"], "2"]}]], 
     StyleBox["a",
      FontWeight->"Bold"]}]}], TraditionalForm]]],
 ", this is known as the vector component of u along a. "
}], "Subitem",
 CellChangeTimes->{{3.511870612744873*^9, 3.511870674917819*^9}, {
  3.511969675165084*^9, 3.5119696764690447`*^9}}],

Cell[TextData[{
 StyleBox["u-",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["proj", "a"], 
     StyleBox["u",
      FontWeight->"Bold"]}], 
    StyleBox["=",
     FontWeight->"Bold"], 
    RowBox[{"u", "-", 
     RowBox[{
      FractionBox[
       StyleBox[
        RowBox[{"u", "\[CenterDot]", "a"}],
        FontWeight->"Bold"], 
       RowBox[{"||", 
        StyleBox["a",
         FontWeight->"Bold"], 
        SuperscriptBox[
         StyleBox["||",
          FontWeight->"Bold"], "2"]}]], 
      StyleBox["a",
       FontWeight->"Bold"]}]}]}], TraditionalForm]]],
 ", this is known as the vector projection of ",
 StyleBox["v ",
  FontWeight->"Bold"],
 " on ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["a", "\[Perpendicular]"], TraditionalForm]]],
 ". (NOTE IN MATHEMATICA \[Perpendicular] is made by esc, perp, esc)"
}], "Subitem",
 CellChangeTimes->{{3.511870677063611*^9, 3.511870720894334*^9}, {
  3.511870799927805*^9, 3.5118708182445507`*^9}, {3.511870937802348*^9, 
  3.511870937810131*^9}}],

Cell[TextData[{
 "The distance between a point and a line is given by the following equation: \
",
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", 
    FractionBox[
     RowBox[{"|", " ", 
      RowBox[{
       SubscriptBox["ax", "0"], "+", 
       SubscriptBox["by", "0"], "+", "c"}], " ", "|"}], 
     SqrtBox[
      RowBox[{
       SuperscriptBox["a", "2"], "+", 
       SuperscriptBox["b", "2"]}]]]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.5118709380262823`*^9, 3.511871041927496*^9}}],

Cell[TextData[{
 "the Distance between a point and a plane is D=",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"|", " ", 
     RowBox[{
      SubscriptBox["ax", "0"], "+", 
      SubscriptBox["by", "0"], "+", 
      SubscriptBox["cz", "0"], "+", "d"}], " ", "|"}], 
    SqrtBox[
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      SuperscriptBox["b", "2"], "+", 
      SuperscriptBox["c", "2"]}]]], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.5118710423921127`*^9, 3.5118710761580544`*^9}}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["3.4 The Geometry of Linear Systems", "Subsection",
 CellChangeTimes->{{3.511820407922785*^9, 3.5118204158831*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      StyleBox["x",
       FontWeight->"Bold"], "0"], "and", " ", 
     StyleBox["v",
      FontWeight->"Bold"], " ", "are", " ", "vectors", " ", "in", " ", 
     SuperscriptBox["R", "n"]}], ",", " ", 
    RowBox[{"adn", " ", "if", " ", 
     StyleBox["v",
      FontWeight->"Bold"], " ", "is", " ", "nonzero"}], ",", " ", 
    RowBox[{
     RowBox[{"then", " ", "the", " ", "equatoin", " ", 
      StyleBox["x",
       FontWeight->"Bold"]}], "=", 
     RowBox[{
      SubscriptBox[
       StyleBox["x",
        FontWeight->"Bold"], "o"], "+", 
      RowBox[{
       StyleBox[
        RowBox[{"t", 
         StyleBox["v",
          FontWeight->"Bold"]}]], " ", "defines", " ", "a", " ", "line", " ", 
       "through", " ", 
       SubscriptBox[
        StyleBox["x",
         FontWeight->"Bold"], "0"]}]}]}]}], TraditionalForm]]],
 "that is parallel to ",
 StyleBox["v. ",
  FontWeight->"Bold"]
}], "Subitem",
 CellChangeTimes->{{3.5118711257701597`*^9, 3.511871231723979*^9}, 
   3.511969682469569*^9}],

Cell[TextData[{
 "If ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      StyleBox["x",
       FontWeight->"Bold"], 
      RowBox[{"0", ","}]], " ", 
     SubscriptBox[
      StyleBox["v",
       FontWeight->"Bold"], "1"]}], ",", " ", 
    RowBox[{"and", " ", 
     SubscriptBox[
      StyleBox["v",
       FontWeight->"Bold"], "2"], " ", "are", " ", "vectors", " ", "in", " ", 
     
     SuperscriptBox["R", "n"]}], ",", " ", 
    RowBox[{"and", " ", "if", " ", 
     SubscriptBox[
      StyleBox["v",
       FontWeight->"Bold"], "1"], "and", " ", 
     SubscriptBox[
      StyleBox["v",
       FontWeight->"Bold"], "2"], " ", "are", " ", "nonzero"}], ",", " ", 
    RowBox[{
     RowBox[{"then", " ", "the", " ", "equation", " ", 
      StyleBox["x",
       FontWeight->"Bold"]}], "=", 
     RowBox[{
      SubscriptBox[
       StyleBox["x",
        FontWeight->"Bold"], "o"], "+", 
      RowBox[{
       SubscriptBox["t", "1"], 
       SubscriptBox[
        StyleBox["v",
         FontWeight->"Bold"], "1"]}], "+", 
      RowBox[{
       SubscriptBox["t", "2"], 
       SubscriptBox[
        StyleBox["v",
         FontWeight->"Bold"], "2"], " ", "defines", " ", "a", " ", "plane", 
       " ", "through", " ", 
       SubscriptBox[
        StyleBox["x",
         FontWeight->"Bold"], "0"], "that", " ", "is", " ", "paralel", " ", 
       "to", 
       RowBox[{
        FormBox[
         RowBox[{
          SubscriptBox[
           StyleBox["v",
            FontWeight->"Bold"], "1"], "and", " ", 
          SubscriptBox[
           StyleBox["v",
            FontWeight->"Bold"], "2"]}],
         TraditionalForm], 
        StyleBox[".",
         FontWeight->"Bold"], 
        StyleBox[" ",
         FontWeight->"Bold"]}]}]}]}]}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.5118712318398647`*^9, 3.511871302397767*^9}, {
  3.511968689840454*^9, 3.511968692112306*^9}}]
}, Open  ]]
}, Open  ]],

Cell[CellGroupData[{

Cell["3.5 Cross Products", "Subsection",
 CellChangeTimes->{{3.511820418506999*^9, 3.511820421650947*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 "If ",
 StyleBox["u ",
  FontWeight->"Bold"],
 " and ",
 StyleBox["v",
  FontWeight->"Bold"],
 " are vectors in 3-space, then the cross product is defined as this ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"determinant", "."}], " ", "|", GridBox[{
      {"i", "j", "k"},
      {
       SubscriptBox["u", "1"], 
       SubscriptBox["u", "2"], 
       SubscriptBox["u", "3"]},
      {
       SubscriptBox["v", "1"], 
       SubscriptBox["v", "2"], 
       SubscriptBox["v", "3"]}
     }], "|"}], TraditionalForm]]]
}], "Subitem",
 CellChangeTimes->{{3.511871472656968*^9, 3.511871545633902*^9}, {
  3.5118719923958473`*^9, 3.5118719924033527`*^9}, {3.511872033276472*^9, 
  3.5118720343560963`*^9}}],

Cell[TextData[{
 "If ",
 StyleBox["u, v, w",
  FontWeight->"Bold"],
 " are all vectors in 3-space, then ",
 StyleBox["u\[CenterDot](v \[Cross] w)",
  FontWeight->"Bold"],
 " is called the scalar triple product.  It is equal to the volume of the \
parallelepiped determined by the vectors ",
 StyleBox["u,v,",
  FontWeight->"Bold"],
 " and ",
 StyleBox["w.",
  FontWeight->"Bold"]
}], "Subitem",
 CellChangeTimes->{{3.511871995036277*^9, 3.511872048515174*^9}, {
  3.51196876230932*^9, 3.5119687665428057`*^9}, {3.5119687981346903`*^9, 
  3.511968817869569*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["Cross product Algebra", "Subsubsection",
 CellChangeTimes->{{3.511871553982367*^9, 3.5118715582170057`*^9}}],

Cell[CellGroupData[{

Cell[TextData[{
 StyleBox["u\[CenterDot](u \[Cross] v)",
  FontWeight->"Bold"],
 "=",
 StyleBox["0",
  FontWeight->"Bold"]
}], "Subitem",
 CellChangeTimes->{{3.511871564997044*^9, 3.511871588306594*^9}}],

Cell[TextData[StyleBox["v\[CenterDot](u \[Cross] v)=0",
 FontWeight->"Bold"]], "Subitem",
 CellChangeTimes->{{3.5118715892690983`*^9, 3.511871605225132*^9}, {
  3.511968876991043*^9, 3.5119688776123466`*^9}}],

Cell[TextData[{
 StyleBox["||u \[Cross] ",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     StyleBox["v",
      FontWeight->"Bold"], 
     SuperscriptBox["||", "2"]}], "=", 
    RowBox[{"||", 
     StyleBox["u",
      FontWeight->"Bold"], 
     SuperscriptBox["||", "2"], "||", 
     StyleBox["v",
      FontWeight->"Bold"], 
     SuperscriptBox["||", "2"], 
     RowBox[{"-", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         StyleBox["u",
          FontWeight->"Bold"], "\[CenterDot]", 
         StyleBox["v",
          FontWeight->"Bold"]}], 
        StyleBox[" ",
         FontWeight->"Bold"], ")"}], "2"]}]}]}], TraditionalForm]]],
 " - Lagrange\[CloseCurlyQuote]s Identity"
}], "Subitem",
 CellChangeTimes->{{3.5118716091961937`*^9, 3.511871661866104*^9}}],

Cell[TextData[StyleBox["u \[Cross] (v \[Cross] w) = (u\[CenterDot]w) v - (u\
\[CenterDot]v) w",
 FontWeight->"Bold"]], "Subitem",
 CellChangeTimes->{{3.511871664603016*^9, 3.511871693048202*^9}, {
  3.511871735857584*^9, 3.511871741913492*^9}, {3.511968942627316*^9, 
  3.511968944603258*^9}}],

Cell[TextData[StyleBox["(u \[Cross] v) \[Cross] w = (u\[CenterDot]w) v - (v\
\[CenterDot]w) u",
 FontWeight->"Bold"]], "Subitem",
 CellChangeTimes->{{3.5118716937624607`*^9, 3.511871731377904*^9}, 
   3.511968937951721*^9}],

Cell[TextData[{
 StyleBox["u \[Cross] v= ",
  FontWeight->"Bold"],
 "-(",
 StyleBox["v \[Cross] u)",
  FontWeight->"Bold"]
}], "Subitem",
 CellChangeTimes->{{3.511871752304906*^9, 3.511871771329399*^9}}],

Cell[TextData[StyleBox["u \[Cross](v+w)=(u\[Cross]v)+(u\[Cross]w)",
 FontWeight->"Bold"]], "Subitem",
 CellChangeTimes->{{3.511871774962141*^9, 3.5118717980057373`*^9}}],

Cell[TextData[StyleBox["(u+v) \[Cross] w =(u \[Cross] w)+( v \[Cross] w)",
 FontWeight->"Bold"]], "Subitem",
