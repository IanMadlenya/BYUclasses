  FormBox[GridBox[{
     {
      RowBox[{
       RowBox[{"<", 
        SubscriptBox["u", "1"]}], ",", 
       RowBox[{
        SubscriptBox["v", "1"], ">"}]}], 
      RowBox[{
       RowBox[{"<", 
        SubscriptBox["u", "2"]}], ",", 
       SubscriptBox["v", 
