@article{yanagisawa:1988,
    title   ={Properties of Pb-Doped Bi-Sr-Ca-Cu-O Superconductors},
    author  ={Yanagisawa, E. and Dietderich, D.R. and Kumakura, H. and Togano, K. and Maeda, H. and Takahashi, K.},
    journal ={Japanese J APPL PHYS},
    volume  ={27},
    number  ={8},
    pages   ={L1460--L1462},
    year    ={1988},
    annote = {This paper explains the construction and testing of Pb-doped
              Bi-Sr-Ca-Cu-O superconductors. different methods for creating
              the superconductors were used, and those different methods
              produced different results. The end findings were that
              conventional powder solid-state reaction doping led to the
              highest zero resistance temperature with a value of 108 K.}
}

@article{Green:1989,
    Abstract = {Presents the results of a systematic study on compositional variations within the (Bi,Pb)[sub2]Sr[sub2]Ca[sub2]Cu[sub3]O[subδ] superconducting system. Superconducting phases that have been identified in the system; Details on the 2223 polytypoid; Preliminary results of transport critical current measurements at liquid-nitrogen temperature in zero applied field; Experimental procedure.},
    Author   = {Green, S. M. and Mei, Yu and Manzi, A. E. and Luo, H. L. and Ramesh, R. and Thomas, G.},
    ISSN     = {00218979},
    Journal  = {J APPL PHYS},
    Keywords = {SUPERCONDUCTIVITY, NITROGEN, TEMPERATURE},
    Number   = {2},
    Pages    = {728},
    Title    = {Effects of compositional variations on the properties of superconducting (Bi,Pb)2Sr2Ca2Cu3Oδ.},
    Volume   = {66},
    URL      = {https://www-lib-byu-edu.erl.lib.byu.edu/cgi-bin/remoteauth.pl?url=http://search.ebscohost.com.erl.lib.byu.edu/login.aspx?direct=true&db=iih&AN=7632059&site=ehost-live&scope=site},
    Year     = {1989},
    annote   = {This paper was among the first to examine the properties of
                different compositional makeups of the BSCCO superconductor
                family. As the relative composition shares of Bi and Sr varied,
                they witnessed widely different results.
                They found that the phase 2223 makeup was the most
                structurally stable compound that exhibited a critical
                superconductivity temperature above 100 K.}
    }


@article{anis:2009,
  title     = {Thermal and electrothermal characterization of bismuth based high-Tc superconductors},
  author    = {Anis Rehman, M.},
  journal   = {J ALLOY COMPD},
  volume    = {469},
  number    = {1},
  pages     = {66--72},
  year      = {2009},
  publisher = {Elsevier},
  annote    = {This recent study examined the thermal and electric properties
               of a BSCCO superconductor. This is the same family of
               superconductor that we used in our experiments. The authors
               examined the dc electrical resistivity, ac susceptibility,
               thermal transport, electrothermal conductivity, and
               thermoelectric properties all as a function of temperature.
               Their findings showed that the transition temperature for
               superconductivity was about 100 K.}
}


@article{Ashburn:1987,
  title     = {Superconductivity at 93 K in a new mixed-phase Y-Ba-Cu-O compound system at ambient pressure},
  author    = {Wu, M. K. and Ashburn, J. R. and Torng, C. J. and Hor, P. H. and Meng, R. L. and Gao, L. and Huang, Z. J. and Wang, Y. Q. and Chu, C. W.},
  journal   = {PHYS. REV. LETT.},
  volume    = {58},
  issue     = {9},
  pages     = {908--910},
  year      = {1987},
  month     = {Mar},
  doi       = {10.1103/PhysRevLett.58.908},
  url       = {http://link.aps.org/doi/10.1103/PhysRevLett.58.908},
  publisher = {American Physical Society},
  annote    = {This paper was among the first to show superconductivity at
               temperatures above the boiling point of liquid nitrogen (77K).
               They used a Y-Ba-Cu-O compound system and showed
               superconductivity at 93 K. Another important contribution from
               this paper was that it presented the first substantial evidence
               that superconductivity could occur above 100 K.}
}
