 RowBox[{
  RowBox[{"%", "//", "FullForm"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"Graphics", "[", 
  RowBox[{"{", 
   RowBox[{"g1", ",", "g2"}], "}"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"%", "//", "FullForm"}]}], "Input",
 CellChangeTimes->{
  3.4307473007925863`*^9, {3.4307484755495167`*^9, 3.430748729195011*^9}, 
   3.4307487683676386`*^9, {3.430753767027424*^9, 3.430753818277424*^9}, {
   3.4307539843242984`*^9, 3.430753996339924*^9}, {3.4307550525430484`*^9, 
