  RowBox[{"cpilength", " ", "=", " ", 
   RowBox[{"Length", "[", "cpiaverage", "]"}]}], ";", " ", 
  RowBox[{"cpiPlot", " ", "=", " ", 
   RowBox[{"cpiaverage", "[", 
    RowBox[{"[", 
     RowBox[{"144", ";;", "cpilength"}], "]"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"M1SL", " ", "=", " ", 
   RowBox[{
    RowBox[{"Transpose", "[", 
     RowBox[{"Import", "[", "\"\<M1SL (1).csv\>\"", "]"}], "]"}], "[", 
    RowBox[{"[", "3", "]"}], "]"}]}], ";", " ", 
  RowBox[{"M1SLlength", " ", "=", " ", 
   RowBox[{"Length", "[", "M1SL", "]"}]}], ";", " ", 
  RowBox[{"M1SLplot", " ", "=", " ", 
   RowBox[{"M1SL", "[", 
    RowBox[{"[", 
     RowBox[{"120", ";;", "M1SLlength"}], "]"}], "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.5361907441327972`*^9, 3.536190813176401*^9}, {
  3.5361908484558773`*^9, 3.536190849182262*^9}, {3.536190887088109*^9, 
