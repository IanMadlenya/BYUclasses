Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["v", "0"], " ", "=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["A", 
        RowBox[{"1", "+", "r"}]], "+", 
       FractionBox["A", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "+", "r"}], ")"}], "t"]], "+", 
