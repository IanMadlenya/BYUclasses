 FormBox[
  RowBox[{
   RowBox[{"l", " ", "+", "h"}], " ", "=", " ", "24"}], 
  TraditionalForm]], "EquationNumbered",
 CellChangeTimes->{{3.526320779559986*^9, 3.526320783909638*^9}, 
   3.526320856643819*^9},
 TextAlignment->Center],

