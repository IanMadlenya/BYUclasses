      RowBox[{"\[PartialD]", "w"}], 
      RowBox[{"\[PartialD]", "L"}]]}]}], "=", 
   RowBox[{
    SubscriptBox["ME", "L"], " ", "=", " ", 
    RowBox[{
     SubscriptBox["MRP", "L"], " ", "=", " ", 
     RowBox[{"P", "*", " ", 
