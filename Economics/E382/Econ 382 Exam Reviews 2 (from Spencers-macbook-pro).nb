Cell["Example: Inter-temporal Impatience", "Subsubsection",
 CellChangeTimes->{{3.5263003119615383`*^9, 3.526300324775846*^9}, 
   3.5263214431631527`*^9}],

Cell[BoxData[
 FormBox[
  RowBox[{"W", " ", "=", " ", 
   RowBox[{"=", " ", 
    RowBox[{
     RowBox[{
      SubscriptBox["c", "0"], "+", 
