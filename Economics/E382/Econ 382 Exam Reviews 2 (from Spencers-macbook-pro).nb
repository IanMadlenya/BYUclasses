    RowBox[{"w", " ", "+", " ", 
     RowBox[{"L", " ", 
      FractionBox[
       RowBox[{"\[PartialD]", "w"}], 
       RowBox[{"\[PartialD]", "L"}]]}]}]}]}], 
  TraditionalForm]], "EquationNumbered",
